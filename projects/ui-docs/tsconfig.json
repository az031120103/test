{
  "compilerOptions": {
    "target": "esnext",
    "outDir": "docs/.vitepress/dist/",
    "module": "esnext",
    "moduleResolution": "node",
    "strict": true,
    "noLib": false,
    "forceConsistentCasingInFileNames": true,
    "allowSyntheticDefaultImports": true,
    "strictFunctionTypes": false,
    "jsx": "preserve",
    "baseUrl": "./",
    "allowJs": true,
    "sourceMap": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "experimentalDecorators": true,
    "declaration": true,
    "isolatedModules": true,
    "lib": [
      "dom",
      "esnext"
    ],
    "noImplicitAny": false,
    "skipLibCheck": true,
    "removeComments": true,
    "types": [
      "node"
    ]
  },
  "include": [
    "docs/**/*.ts",
    "docs/**/*.d.ts",
    "docs/**/*.tsx",
    "docs/**/*.vue"
  ],
  "exclude": [
    "node_modules",
    "docs/.vitepress/dist",
    "**/*.js",
    "**/*.md"
  ]
}
